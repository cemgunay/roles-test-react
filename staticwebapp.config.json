{
    "navigationFallback": {
        "rewrite": "/index.html"
    },
    "routes": [
        {
            "route": "/admin",
            "allowedRoles": [ "admin" ]
        },
        {
            "route": "/beast",
            "allowedRoles": [ "Beast.Mode" ]
        },
        
        {
            "route": "/authenticated",
            "allowedRoles": [ "authenticated" ]
        }
    ],
    "auth": {
        "rolesSource": "/api/appRolesReact",
        "identityProviders": {
            "azureActiveDirectory": {
                "userDetailsClaim": "http://schemas.xmlsoap.org/ws/2005/05/identity/claims/name",
                "registration": {
                    "openIdIssuer": "https://approlesdemob2c.b2clogin.com/approlesdemob2c.onmicrosoft.com/B2C_1_approlesdemob2c/v2.0",
                    "clientIdSettingName": "AAD_CLIENT_ID",
                    "clientSecretSettingName": "AAD_CLIENT_SECRET"
                },
                "login": {
                    "loginParameters": [
                        "resource=https://graph.microsoft.com"
                    ]
                }
            }
        }
    },
    "globalHeaders": {
        "Cache-Control": "no-cache"
    }
}