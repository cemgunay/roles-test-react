{
    "navigationFallback": {
        "rewrite": "/index.html"
    },
    "routes": [
        {
            "route": "/",
            "allowedRoles": [ "anonymous", "authenticated" ]
        },
        {
            "route": "/admin",
            "allowedRoles": [ "admin" ]
        },
        {
            "route": "/beast",
            "allowedRoles": [ "Beast.Mode" ]
        },
        
        {
            "route": "/authenticated",
            "allowedRoles": [ "authenticated" ]
        }
    ],
    "globalHeaders": {
        "Cache-Control": "no-cache"
    },
    "platform": {
        "apiRuntime": "node:18"
    }
}